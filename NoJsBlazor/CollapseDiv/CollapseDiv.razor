@namespace NoJsBlazor

<div class="collapse-div @(Collapsed ? "collapsed" : "collapseable")" @attributes="@Attributes">
    <button class="head" @onmousedown="touchClick.OnMouseDown" @ontouchstart="touchClick.OnTouchStart">
        @Head
    </button>
    <div class="content">
        @Content
    </div>
</div>
