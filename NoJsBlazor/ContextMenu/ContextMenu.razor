@namespace NoJsBlazor
@inherits ListholdingComponentBase<ContextSubMenu>

<ul class="context-menu @(Expanded ? "expanded" : string.Empty)" @attributes="@Attributes" style="top: @(top)px; left: @(left)px; @AddStyles()">
